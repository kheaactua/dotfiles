layout {
    cwd "/"
    tab name="Tab #1" hide_floating_panes=true {
        pane cwd="/home/matt/workspace/arch-setup/git-remote-gcrypt" size="50%"
        pane size="50%" split_direction="vertical" {
            pane command="/home/matt/.local/share/bob/nightly/bin/nvim" cwd="/home/matt/workspace" size="50%" {
                args "/home/matt/.config/zellij/config.kdl"
                start_suspended true
            }
            pane command="tail" cwd="/tmp/zellij-1000/zellij-log" size="50%" {
                args "-f" "zellij.log"
                start_suspended true
            }
        }
        pane size=1 borderless=true {
            plugin location="zellij:compact-bar"
        }
    }
    tab name="covesa" hide_floating_panes=true {
        pane size="50%" split_direction="vertical" {
            pane cwd="/home/matt/workspace/vsomeip" size="50%"
            pane cwd="/home/matt/dotfiles/dotfiles-secret/ford/keys/gpg" size="50%"
        }
        pane size="50%" split_direction="vertical" {
            pane command="/home/matt/.local/share/bob/nightly/bin/nvim" cwd="/home/matt/dotfiles/dotfiles-secret" size="50%" {
                args "setup_dotfiles.sh"
                start_suspended true
            }
            pane cwd="/home/matt/dotfiles/stow/gnupg/.gnupg" size="50%"
        }
        pane size=1 borderless=true {
            plugin location="zellij:compact-bar"
        }
    }
    tab name="phx-fsb" hide_floating_panes=true {
        pane cwd="/f/phoenix"
        pane size=1 borderless=true {
            plugin location="zellij:compact-bar"
        }
    }
    tab name="proxy" focus=true hide_floating_panes=true {
    }
    tab name="Tab #5" hide_floating_panes=true {
